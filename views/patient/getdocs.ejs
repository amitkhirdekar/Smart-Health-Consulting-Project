<%- include("../partials/header.ejs") %>


<body class="text-gray-500 bg-gray-900 body-font">

  <section class="text-gray-500 bg-gray-900 body-font">
      <div class="container px-5 py-24 mx-auto">
          <div class="text-center mb-20">
              <h1 class="sm:text-8xl text-3xl font-medium text-center title-font text-white mb-4">Welcome to SHCS</h1>
              <p class="text-base xl:w-2/4 lg:w-3/4 mx-auto text-3xl leading-tight">SHC is an integrated software aims
                  to maintain patient health records, real time scheduling of appointments and predicting disease
                  using symptoms. </p>
          </div>


          <div class="flex flex-wrap lg:w-4/5 sm:mx-auto sm:mb-2 -mx-2">
              <div class="mx-auto p-2 sm:w-1/3 w-full">
                  <a href="/patient">
                      <div class="bg-gray-800 rounded flex p-4 h-full items-center hover:bg-indigo-600">
                          <svg fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"
                              stroke-width="3" class="text-indigo-500 w-6 h-6 flex-shrink-0 mr-4" viewBox="0 0 24 24">
                              <path d="M22 11.08V12a10 10 0 11-5.93-9.14"></path>
                              <path d="M22 4L12 14.01l-3-3"></path>
                          </svg>
                          <span class="title-font font-medium text-white"> My Dashboard </span>
                      </div>
              </div>
              </a>
          </div>
  </section>

  <div class="lg:w-3/4 md:w-2/3 mx-auto">
      <div class="flex -m-2"></div>
      <div class="p-2 w-full pt-0 mt-0 border-t border-gray-800 text-center">
      </div>
  </div>
  </div>

<section class="text-gray-500 bg-gray-900 body-font overflow-hidden">
  <div class="container px-5 pb-0 pt-12 mx-auto">
    <% doctors.forEach(function(doctor){ %>
    <div class="lg:w-4/5 mx-auto flex flex-wrap">
      <img alt="Doctor" class="lg:w-1/4 ml-auto w-full lg:h-auto h-64 object-cover object-center rounded" src="../images/newDocAvtar.jpg">
      <div class="lg:w-1/2 w-full lg:pl-10 lg:py-6 mt-6 lg:mt-0 mr-auto">
        <h2 class="text-2xl title-font text-gray-600 tracking-widest"> Doctor. </h2>
        <h1 class="text-white text-5xl title-font font-medium mb-1"> <%= doctor.name %> </h1>
        <div class="flex mb-4 mt-2">
          <span class="flex items-center">
            <svg fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10 text-indigo-500" viewBox="0 0 24 24">
              <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"></path>
            </svg>
            <svg fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10 text-indigo-500" viewBox="0 0 24 24">
              <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"></path>
            </svg>
            <svg fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10 text-indigo-500" viewBox="0 0 24 24">
              <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"></path>
            </svg>
            <svg fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10 text-indigo-500" viewBox="0 0 24 24">
              <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"></path>
            </svg>
            <svg fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10 text-indigo-500" viewBox="0 0 24 24">
              <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"></path>
            </svg>
            <span class="text-gray-500 ml-3 text-3xl">4 Reviews</span>
          </span>
          <span class="flex ml-3 pl-3 py-2 border-l-2 border-gray-800 text-gray-600">
            <a>
              <svg fill="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10" viewBox="0 0 24 24">
                <path d="M18 2h-3a5 5 0 00-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 011-1h3z"></path>
              </svg>
            </a>
            <a class="ml-2">
              <svg fill="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10" viewBox="0 0 24 24">
                <path d="M23 3a10.9 10.9 0 01-3.14 1.53 4.48 4.48 0 00-7.86 3v1A10.66 10.66 0 013 4s-4 9 5 13a11.64 11.64 0 01-7 2c9 5 20 0 20-11.5a4.5 4.5 0 00-.08-.83A7.72 7.72 0 0023 3z"></path>
              </svg>
            </a>
            <a class="ml-2">
              <svg fill="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10" viewBox="0 0 24 24">
                <path d="M21 11.5a8.38 8.38 0 01-.9 3.8 8.5 8.5 0 01-7.6 4.7 8.38 8.38 0 01-3.8-.9L3 21l1.9-5.7a8.38 8.38 0 01-.9-3.8 8.5 8.5 0 014.7-7.6 8.38 8.38 0 013.8-.9h.5a8.48 8.48 0 018 8v.5z"></path>
              </svg>
            </a>
          </span>
        </div>
        <p class="leading-normal text-3xl"> Famous <%= doctor.degree %> <br> Address : <%= doctor.address %><br>  Description : Gold Medalist </p>
        <div class="flex mt-6 items-center pb-5 border-b-2 border-gray-800 mb-5">
        </div>
        <div class="flex">
          <span class="title-font font-medium text-5xl text-white">â‚¹100.00</span>
          <a href="/doctor/<%= doctor._id %>" class="flex ml-auto">
          <button class="flex ml-auto text-white bg-indigo-500 border-0 py-4 px-10 focus:outline-none hover:bg-indigo-600 rounded text-3xl"> Details About Doctor</button>
        </a>
        </div>
      </div>
    </div>
  </div>
  <div class="mx-auto w-3/4 flex mt-2 items-center pb-5 border-b-2 border-gray-800 mb-5">
  </div>  <% }) %>
</section>


</body>
<%- include("../partials/footer.ejs") %>